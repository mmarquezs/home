gtk-icon-theme-name = "Paper"
gtk-theme-name = "Arc"
gtk-font-name = "DejaVu Sans 11"
style "pcmanfm" {
  base[NORMAL] = "#383C4A"
  base[ACTIVE] = "#383C4A"
  text[NORMAL] = "#BBBBBB"
  bg[NORMAL] = "#383C4A"
  bg[PRELIGHT] = "#383C4A"
  fg[PRELIGHT] = "#383C4A"
  fg[NORMAL] = "#383C4A"
  fg[INSENSITIVE] = "#383C4A"
  bg[INSENSITIVE] = "#383C4A"
}
